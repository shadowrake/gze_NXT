generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI_Read")
}

type CcGames {
  game1 String
  game2 String
  game3 String
}

type CcGear {
  name String
}

type CcLinks {
  instagram String
  twitch    String
  twitter   String
  youtube   String
}

type NewsAuthor {
  imgUrl String
  name   String
}

type SlideshowHeaderSliderBtn {
  aria      String
  dataSlide String
}

model Staff {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  age         String
  img         String
  name        String
  nationality String
  role        String
}

model cc {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  Nationality String
  age         String
  desc        String
  firstname   String
  gamertag    String
  games       CcGames
  gear        CcGear
  img         String
  imgAlt      String
  keyword     String
  lastname    String
  links       CcLinks
  role        String
}

model events {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  datetime DateTime @db.Date
  desc     String
  imgAlt   String
  imgUrl   String
  keyword  String
  title    String
}

model matches {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  date    DateTime @db.Date
  keyword String
  name    String
}

model media {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  imgAlt String
  imgUrl String
}

model news {
  id       String     @id @default(auto()) @map("_id") @db.ObjectId
  author   NewsAuthor
  datetime DateTime   @db.Date
  desc     String
  imgAlt   String
  imgUrl   String
  keyword  String
  title    String
}

model players {
  id      String  @id @default(auto()) @map("_id") @db.ObjectId
  keyword String
  name    String
  team    String?
}

model slideshowHeader {
  id        String                   @id @default(auto()) @map("_id") @db.ObjectId
  href      String
  img       String
  imgAlt    String
  sliderBtn SlideshowHeaderSliderBtn
}

model staffRec {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  keyword String
  name    String
}

model teamStaff {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  age         String
  img         String
  keyword     String
  name        String
  nationality String
  role        String
}

model team_achievements {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  date    DateTime @db.Date
  img     String
  keyword String
  name    String
}

model teams {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  desc       String
  gamer_link String @map("gamer-link")
  imageAlt   String
  imageSrc   String
  name       String
  title      String
}

model teamsSoon {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  desc     String
  href     String?
  imageAlt String
  imageSrc String
  keyword  String
  manager  String
  name     String
  title    String
}

model teamsinput {
  id      String  @id @default(auto()) @map("_id") @db.ObjectId
  dummy   String?
  keyword String
  name    String
}

model users {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}
