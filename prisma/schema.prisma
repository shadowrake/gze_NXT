generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI_Read")
}

type NewsAuthor {
  imgUrl String
  name   String
}

model Staff {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  age         String
  img         String
  name        String
  nationality String
  role        String
}

model events {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model matches {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model media {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model news {
  id       String     @id @default(auto()) @map("_id") @db.ObjectId
  author   NewsAuthor
  date     String
  datetime String
  desc     String
  imgAlt   String
  imgUrl   String
  title    String
}

model players {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  keyword String
  name    String
}

model teams {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  desc       String
  gamer_link String @map("gamer-link")
  imageAlt   String
  imageSrc   String
  name       String
  title      String
}

model teamsSoon {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  href     String?
  imageAlt String
  imageSrc String
  name     String
}
